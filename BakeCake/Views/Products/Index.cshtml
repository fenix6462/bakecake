<div class="row">
    <div class="col-md-12">
        <h1>Produkty</h1>
        <table class="table hover">
            <tr>
                <th>Nazwa</th>
                <th>Cena</th>
                <th>Waga</th>
            </tr>
            <tr ng-repeat="product in products">
                <td>{{ product.Name }}</td>
                <td>{{ product.Price }}</td>
                <td>{{ product.Weight }}</td>
            </tr>
        </table>
    </div>
</div>
